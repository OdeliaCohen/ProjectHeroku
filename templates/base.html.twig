        <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;700&display=swap" rel="stylesheet">
        <style>
            /* Vos styles CSS ici */
            .pink-link {
                color: #FF698D;
            }

            .pink-link:hover {
                color: white;
                background-color: #FF698D;
                border-color: #FF698D;
            }
        </style>
    {% endblock %}
